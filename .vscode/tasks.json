{
    "version": "0.1.0",
    "args": [],
    "showOutput": "always",
    "command": "make",
    "isShellCommand": true,
    "tasks": [
        {
            "taskName": "x86-dbg",
            "isBuildCommand": false,
            "showOutput": "always",
            "args": ["x86-dbg"],
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": ["relative", "${workspaceRoot}"],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            }
        }, 
        {
            "taskName": "onion",
            "isBuildCommand": false,
            "showOutput": "always",
            "args": ["onion"],
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": ["relative", "${workspaceRoot}"],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            }
        },
        {
            "taskName": "clean",
            "isBuildCommand": false,
            "showOutput": "always",
            "args": ["clean"]
        }
    ]
}